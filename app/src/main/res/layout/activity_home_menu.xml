<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/linearColumnrectanglefour"
            style="@style/groupStylewhite_A700"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_37pxv"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <FrameLayout
                android:id="@+id/frameHomeMenu"
                style="@style/groupStylewhite_A700"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="start">

                <LinearLayout
                    android:id="@+id/linearSidebar"
                    style="@style/groupStylewhite_A700"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:orientation="vertical">

                    <FrameLayout
                        android:id="@+id/frameStackrectangleeighteen"
                        android:layout_width="@dimen/_130pxh"
                        android:layout_height="@dimen/_130pxv"
                        android:layout_marginStart="@dimen/_28pxh"
                        android:layout_marginTop="@dimen/_60pxv"
                        android:layout_marginEnd="@dimen/_28pxh">

                        <LinearLayout
                            android:id="@+id/linearProfile"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="left"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/imageRectangleEighteen"
                                cornerRadius="@{@dimen/_10pxh}"
                                imageRes="@{@drawable/img_rectangle4}"
                                android:layout_width="@dimen/_46pxh"
                                android:layout_height="@dimen/_49pxh"
                                android:layout_marginStart="@dimen/_3pxh"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:scaleType="fitXY"
                                tools:ignore="ContentDescription"
                                tools:src="@drawable/img_rectangle4" />

                            <TextView
                                android:id="@+id/txtAndrewWade"
                                style="@style/txtIntersemibold16_1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_17pxv"
                                android:ellipsize="end"
                                android:singleLine="true"
                                android:text="@{homeMenuVM.homeMenuModel.txtAndrewWade}"
                                tools:text="@string/lbl_philip_low" />

                            <FrameLayout
                                android:id="@+id/frameStackdot"
                                android:layout_width="@dimen/_50pxh"
                                android:layout_height="@dimen/_18pxv"
                                android:layout_marginTop="@dimen/_7pxv"
                                android:layout_marginEnd="@dimen/_20pxh">

                                <View
                                    android:id="@+id/viewDot"
                                    android:layout_width="@dimen/_2pxh"
                                    android:layout_height="@dimen/_2pxh"
                                    android:layout_gravity="top|start"
                                    android:layout_marginStart="@dimen/_5pxh"
                                    android:layout_marginTop="@dimen/_1pxv"
                                    android:layout_marginEnd="@dimen/_10pxh"
                                    android:layout_marginBottom="@dimen/_10pxv"
                                    android:background="@drawable/rectangle_bg_gray_400_radius_1" />

                                <TextView
                                    android:id="@+id/txt31hcp"
                                    style="@style/txtInterregular14_4"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:ellipsize="end"
                                    android:gravity="top|start"
                                    android:scrollY="@dimen/_10pxv"
                                    android:singleLine="true"
                                    android:text="@{homeMenuVM.homeMenuModel.txt31hcp}"
                                    tools:text="@string/lbl_hcp" />

                                <TextView
                                    android:id="@+id/hcpvalueText"
                                    style="@style/txtInterregular14_2"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="top|left"
                                    android:ellipsize="end"
                                    android:singleLine="true"
                                    android:text="@{homeMenuVM.homeMenuModel.txtZero}"
                                    tools:text="@string/lbl_00" />
                            </FrameLayout>
                        </LinearLayout>

                    </FrameLayout>

                    <View
                        android:id="@+id/viewLine"
                        android:layout_width="@dimen/_339pxh"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_65pxv"
                        android:background="@color/bluegray_50" />

                    <LinearLayout
                        android:id="@+id/linearRowuser"
                        style="@style/groupStylewhite_A700"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_marginEnd="@dimen/_25pxh"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageUser"
                            android:layout_width="@dimen/_16pxh"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_28pxh"
                            android:layout_marginTop="@dimen/_24pxv"
                            android:layout_marginBottom="@dimen/_24pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_user"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_user" />

                        <TextView
                            android:id="@+id/txtLabel"
                            style="@style/txtInterregular16"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:layout_marginTop="@dimen/_20pxv"
                            android:layout_marginEnd="@dimen/_173pxh"
                            android:layout_marginBottom="@dimen/_24pxv"
                            android:layout_weight="0.75"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="@{homeMenuVM.homeMenuModel.txtLabel}"
                            tools:text="@string/lbl_profile" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearRowcheckmark"
                        style="@style/groupStylewhite_A700"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginEnd="@dimen/_25pxh"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageCheckmark"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginStart="@dimen/_27pxh"
                            android:layout_marginTop="@dimen/_21pxv"
                            android:layout_marginBottom="@dimen/_23pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_checkmark"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_checkmark" />

                        <TextView
                            android:id="@+id/txtLabelOne"
                            style="@style/txtInterregular16"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_19pxh"
                            android:layout_marginTop="@dimen/_20pxv"
                            android:layout_marginEnd="@dimen/_159pxh"
                            android:layout_marginBottom="@dimen/_23pxv"
                            android:layout_weight="0.78"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="@{homeMenuVM.homeMenuModel.txtLabelOne}"
                            tools:text="@string/lbl_courses" />
                    </LinearLayout>

                    <EditText
                        android:id="@+id/etThree"
                        style="@style/etSolid"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginEnd="@dimen/_25pxh"
                        android:background="@drawable/rectangle_bg_white_a700"
                        android:drawableStart="@drawable/img_user_gray_700"
                        android:drawablePadding="@dimen/_17pxh"
                        android:ellipsize="end"
                        android:hint="@string/lbl_statistic"
                        android:inputType="text"
                        android:paddingStart="@dimen/_25pxh"
                        android:paddingTop="@dimen/_20pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingBottom="@dimen/_23pxv"
                        android:singleLine="true"
                        android:text="@={homeMenuVM.homeMenuModel.etThreeValue}"
                        android:textColorHint="@color/bluegray_900"
                        tools:ignore="Autofill" />

                    <View
                        android:id="@+id/viewLineOne"
                        android:layout_width="@dimen/_339pxh"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_17pxv"
                        android:background="@color/bluegray_50" />

                    <TextView
                        android:id="@+id/txtOne"
                        style="@style/txtSolid"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginTop="@dimen/_27pxv"
                        android:layout_marginEnd="@dimen/_25pxh"
                        android:ellipsize="end"
                        android:gravity="center_horizontal|left"
                        android:paddingStart="@dimen/_24pxh"
                        android:paddingTop="@dimen/_21pxv"
                        android:paddingBottom="@dimen/_22pxv"
                        android:singleLine="true"
                        android:text="@{homeMenuVM.homeMenuModel.txtOne}"
                        tools:text="@string/msg_settings_secu" />

                    <TextView
                        android:id="@+id/txtTwo"
                        style="@style/txtSolid"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginEnd="@dimen/_25pxh"
                        android:ellipsize="end"
                        android:gravity="center_horizontal|left"
                        android:paddingStart="@dimen/_24pxh"
                        android:paddingTop="@dimen/_20pxv"
                        android:paddingBottom="@dimen/_23pxv"
                        android:singleLine="true"
                        android:text="@{homeMenuVM.homeMenuModel.txtTwo}"
                        tools:text="@string/msg_terms_of_servic" />

                    <TextView
                        android:id="@+id/valueHole10"
                        style="@style/txtSolid"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginEnd="@dimen/_25pxh"
                        android:ellipsize="end"
                        android:gravity="center_horizontal|left"
                        android:paddingStart="@dimen/_24pxh"
                        android:paddingTop="@dimen/_21pxv"
                        android:paddingBottom="@dimen/_22pxv"
                        android:singleLine="true"
                        android:text="@{homeMenuVM.homeMenuModel.txtThreeOne}"
                        tools:text="@string/lbl_help_center" />

                    <View
                        android:id="@+id/viewLineTwo"
                        android:layout_width="@dimen/_339pxh"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_73pxv"
                        android:background="@color/bluegray_50" />

                    <LinearLayout
                        android:id="@+id/linearLogout"
                        style="@style/groupStylewhite_A700"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageMaximize"
                            android:layout_width="@dimen/_21pxh"
                            android:layout_height="@dimen/_18pxh"
                            android:layout_marginStart="@dimen/_32pxh"
                            android:layout_marginTop="@dimen/_23pxv"
                            android:layout_marginBottom="@dimen/_23pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_maximize"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_maximize" />

                        <TextView
                            android:id="@+id/txtLabelTwo"
                            style="@style/txtInterregular16"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_22pxh"
                            android:layout_marginTop="@dimen/_21pxv"
                            android:layout_marginEnd="@dimen/_202pxh"
                            android:layout_marginBottom="@dimen/_22pxv"
                            android:layout_weight="0.74"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="@{homeMenuVM.homeMenuModel.txtLabelTwo}"
                            tools:text="@string/lbl_log_out" />
                    </LinearLayout>

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                    </FrameLayout>

                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </ScrollView>

    <data>
        <variable name="homeMenuVM" type="com.pgp.app.modules.homemenu.data.viewmodel.HomeMenuVM"/>
    </data>
</layout>
