<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/_10pxh"
            android:orientation="horizontal">

            <View
                android:id="@+id/lineLineOne9"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_1pxv"
                android:layout_gravity="start"
                android:layout_weight="1"
                android:background="@color/bluegray_50" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_marginEnd="@dimen/_24pxh"
            android:gravity="start"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="@dimen/_72pxv"
                android:layout_height="@dimen/_90pxh"
                android:scaleType="fitXY"
                app:srcCompat="@color/bluegray_50" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_12pxh"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_marginBottom="@dimen/_26pxv"
                android:layout_weight="0.71"
                android:gravity="start"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/teecolour1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/interbold"
                    android:text="@string/tee_white"
                    android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                    android:textSize="@dimen/_24pxv" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/_10pxh"
            android:orientation="horizontal">

            <View
                android:id="@+id/lineLineOne16"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_1pxv"
                android:layout_gravity="start"
                android:layout_weight="1"
                android:background="@color/bluegray_50" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.71"
            android:gravity="start"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:gravity="start"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="@dimen/_72pxv"
                    android:layout_height="@dimen/_90pxh"
                    android:scaleType="fitXY"
                    app:srcCompat="@color/material_dynamic_primary80" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_marginBottom="@dimen/_26pxv"
                    android:layout_weight="0.71"
                    android:gravity="start"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/teecolour2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/interbold"
                        android:text="@string/tee_blue"
                        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                        android:textSize="@dimen/_24pxv" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:id="@+id/lineLineOne17"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_1pxv"
                android:layout_gravity="start"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_weight="1"
                android:background="@color/bluegray_50" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:gravity="start"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="@dimen/_72pxv"
                    android:layout_height="@dimen/_90pxh"
                    tools:srcCompat="@color/red_401" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_marginBottom="@dimen/_26pxv"
                    android:layout_weight="0.71"
                    android:gravity="start"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/teecolour3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/interbold"
                        android:text="@string/tee_red"
                        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                        android:textSize="@dimen/_24pxv" />

                </LinearLayout>
            </LinearLayout>

            <View
                android:id="@+id/lineLineOne18"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_1pxv"
                android:layout_gravity="start"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_weight="1"
                android:background="@color/bluegray_50" />

        </LinearLayout>

    </LinearLayout>

    <data>
        <variable name="listtitleRowModel" type="com.pgp.app.modules.courses10.data.model.ListtitleRowModel"/>
    </data>

</layout>
