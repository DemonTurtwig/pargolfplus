<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="recordScore30VM" type="com.pgp.app.modules.recordscore30.data.viewmodel.RecordScore30VM"/>
    </data>
    <LinearLayout android:id="@+id/linearRecordScore3"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageArrowleft"
            android:layout_gravity="start"
            android:layout_height="@dimen/_13pxh"
            android:layout_marginEnd="@dimen/_44pxh"
            android:layout_marginStart="@dimen/_44pxh"
            android:layout_marginTop="@dimen/_5pxv"
            android:layout_width="@dimen/_7pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_arrowleft"
            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
        <TextView android:ellipsize="end" android:id="@+id/txtPlayer"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_58pxh"
            android:layout_marginStart="@dimen/_58pxh"
            android:layout_marginTop="@dimen/_32pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{recordScore30VM.recordScore30Model.txtPlayer}"
            style="@style/txtInterbold20" tools:text="@string/lbl_player"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearProfile"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="wrap_content"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowrectangleten"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/userPicMainPage"
                    android:layout_height="@dimen/_61pxh"
                    android:layout_width="@dimen/_54pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_10pxh}"
                    imageRes="@{@drawable/img_rectangle4}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle4"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumntitle"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_31pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_weight="0.59"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/club1"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{recordScore30VM.recordScore30Model.txtTitle}"
                        style="@style/txtIntersemibold16_1" tools:text="@string/lbl_philip_low"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearIndex"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/hcpvalueText"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{recordScore30VM.recordScore30Model.txtZero}"
                            style="@style/txtInterregular14_2" tools:text="@string/lbl_00"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txt31hcp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_3pxh"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{recordScore30VM.recordScore30Model.txt31hcp}"
                            style="@style/txtInterregular14_3" tools:text="@string/lbl_hcp"/>
                        <View
                            android:background="@drawable/rectangle_bg_gray_400_radius_1"
                            android:id="@+id/viewDot"
                            android:layout_height="@dimen/_2pxh"
                            android:layout_marginBottom="@dimen/_7pxv"
                            android:layout_marginStart="@dimen/_4pxh"
                            android:layout_marginTop="@dimen/_7pxv" android:layout_width="@dimen/_2pxh"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <View android:background="@color/white_A700"
                android:id="@+id/lineLine"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_8pxv" android:layout_width="@dimen/_333pxh"/>
        </LinearLayout>
        <View android:background="@color/bluegray_50"
            android:id="@+id/lineLineOne"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_27pxv" android:layout_width="@dimen/_378pxh"/>
        <FrameLayout android:gravity="start"
            android:id="@+id/frameStacktitlebar"
            android:layout_height="@dimen/_515pxv"
            android:layout_marginBottom="@dimen/_20pxv" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearTop"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearTitleBar"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_3pxh"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_A700">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTitleBar"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_54pxh"
                        android:layout_marginStart="@dimen/_54pxh"
                        android:layout_marginTop="@dimen/_21pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{recordScore30VM.recordScore30Model.txtTitleBar}"
                        style="@style/txtInterbold20" tools:text="@string/lbl_score"/>
                    <View android:background="@color/white_A700"
                        android:id="@+id/lineLineTwo"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_390pxh"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowlocation"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_185pxh"
                    android:layout_marginStart="@dimen/_58pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageLocation"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_location"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_location"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLanguage"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.55"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{recordScore30VM.recordScore30Model.txtLanguage}"
                        style="@style/txtInterbold32" tools:text="@string/lbl_hole2"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/valuePar10"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_weight="0.33"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{recordScore30VM.recordScore30Model.txtThree}"
                        style="@style/txtInterbold32_1" tools:text="@string/lbl_03"/>
                </LinearLayout>
                <View android:background="@color/bluegray_50"
                    android:id="@+id/lineLineThree"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_39pxv" android:layout_width="@dimen/_378pxh"/>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearRowstrokes"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_31pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylewhite_A700">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtStrokes"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_19pxv"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_marginTop="@dimen/_22pxv"
                        android:layout_weight="0.37"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{recordScore30VM.recordScore30Model.txtStrokes}"
                        style="@style/txtIntersemibold18" tools:text="@string/lbl_strokes"/>
                    <LinearLayout android:id="@+id/linearRowshape"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_14pxv"
                        android:layout_marginEnd="@dimen/_8pxh"
                        android:layout_marginStart="@dimen/_144pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_weight="0.63"
                        android:layout_width="0dp" android:orientation="horizontal">

                        <LinearLayout
                            android:id="@+id/linearMinus"
                            android:layout_width="@dimen/_32pxv"
                            android:layout_height="@dimen/_32pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:background="@drawable/rectangle_border_gray_400_radius_16"
                            android:gravity="center|center_horizontal"
                            android:orientation="vertical">

                            <View
                                android:id="@+id/viewShape"
                                android:layout_width="@dimen/_10pxh"
                                android:layout_height="@dimen/_1pxv"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginStart="@dimen/_11pxh"
                                android:layout_marginTop="@dimen/_15pxv"
                                android:layout_marginEnd="@dimen/_11pxh"
                                android:layout_marginBottom="@dimen/_15pxv"
                                android:background="@color/gray_700" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/btnTwelve"
                            android:layout_width="@dimen/_36pxh"
                            android:layout_height="@dimen/_36pxh"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:background="@drawable/rectangle_bg_bluegray_50_radius_8"
                            android:ellipsize="end"
                            android:gravity="center"
                            android:paddingStart="@dimen/_9pxh"
                            android:paddingTop="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_9pxh"
                            android:paddingBottom="@dimen/_8pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_12"
                            android:textAllCaps="false"
                            android:textColor="@color/black" />

                        <ImageButton
                            android:id="@+id/btnPlus"
                            android:layout_width="@dimen/_32pxh"
                            android:layout_height="@dimen/_32pxh"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:background="@drawable/rectangle_border_gray_400_radius_16"
                            android:paddingHorizontal="@dimen/_10pxh"
                            android:paddingVertical="@dimen/_10pxv"
                            android:scaleType="fitCenter"
                            android:src="@drawable/img_plus"
                            tools:ignore="ContentDescription" />
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearRowhowmanywerep"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylewhite_A700">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHowmanywerep"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_18pxv"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_marginTop="@dimen/_23pxv"
                        android:layout_weight="0.45"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{recordScore30VM.recordScore30Model.txtHowmanywerep}"
                        style="@style/txtIntersemibold18" tools:text="@string/lbl_par2"/>

                    <TextView
                        android:id="@+id/btnTwo"
                        android:layout_width="@dimen/_36pxh"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_marginStart="@dimen/_225pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_marginEnd="@dimen/_62pxh"
                        android:layout_marginBottom="@dimen/_14pxv"
                        android:background="@drawable/rectangle_bg_bluegray_50_radius_8"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:paddingStart="@dimen/_13pxh"
                        android:paddingTop="@dimen/_8pxv"
                        android:paddingEnd="@dimen/_13pxh"
                        android:paddingBottom="@dimen/_8pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_2"
                        android:textAllCaps="false"
                        android:textColor="@color/black" />
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearRowpenalties"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txtPenalties"
                        style="@style/txtIntersemibold18"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_20pxv"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_weight="0.57"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@{recordScore30VM.recordScore30Model.txtPenalties}"
                        tools:text="@string/lbl_index" />

                    <TextView
                        android:id="@+id/btnTwoOne"
                        android:layout_width="@dimen/_36pxh"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_marginStart="@dimen/_207pxh"
                        android:layout_marginEnd="@dimen/_50pxh"
                        android:background="@drawable/rectangle_bg_bluegray_50_radius_8"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:paddingStart="@dimen/_13pxh"
                        android:paddingTop="@dimen/_8pxv"
                        android:paddingEnd="@dimen/_13pxh"
                        android:paddingBottom="@dimen/_8pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_2"
                        android:textAllCaps="false"
                        android:textColor="@color/black" />
                </LinearLayout>
                <View android:background="@color/bluegray_50"
                    android:id="@+id/lineRectangle"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_115pxv" android:layout_width="@dimen/_353pxh"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearBottom"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_A700">

                <View
                    android:id="@+id/lineLineFive"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_gravity="start"
                    android:background="@color/bluegray_50" />
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnGoToNextHoleOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_9pxh"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_15pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_go_to_next_hole"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_7" tools:text="@string/lbl_go_to_next_hole"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
