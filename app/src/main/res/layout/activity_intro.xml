<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="introVM" type="com.pgp.app.modules.intro.data.viewmodel.IntroVM"/>
    </data>
    <FrameLayout android:id="@+id/frameIntro"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnplay"
            android:layout_gravity="top|center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_39pxh"
            android:layout_marginStart="@dimen/_39pxh"
            android:layout_marginTop="@dimen/_163pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearRowplay"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imagePlay"
                    android:layout_height="@dimen/_52pxh"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:layout_width="@dimen/_27pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_play"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_play"/>
                <ImageView android:id="@+id/imagePpplogointro"
                    android:layout_height="@dimen/_63pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_ppplogointro}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ppplogointro"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageIPhone13Pro"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_372pxh"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_47pxv"
                android:layout_width="@dimen/_304pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_iphone13pro}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_iphone13pro"/>
        </LinearLayout>
    </FrameLayout>
</layout>
