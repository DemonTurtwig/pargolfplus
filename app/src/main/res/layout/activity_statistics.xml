                                    android:text="@{statisticsVM.statisticsModel.txtSixtyThree}"
                                    style="@style/txtIntersemibold14_3" tools:text="@string/lbl_63"/>
                            </LinearLayout>
                            <View android:background="@color/red_400"
                                android:id="@+id/lineLineTwelve"
                                android:layout_gravity="bottom|end"
                                android:layout_height="@dimen/_32pxv"
                                android:layout_marginEnd="@dimen/_24pxh"
                                android:layout_marginStart="@dimen/_24pxh"
                                android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_1pxh"/>
