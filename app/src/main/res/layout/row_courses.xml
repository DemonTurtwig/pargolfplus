<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable name="listtitleRowModel" type="com.pgp.app.modules.courses10.data.model.ListtitleRowModel"/>
    </data>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/linearColumnline"
            style="@style/groupStylewhite_A700"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <View
                android:id="@+id/lineLineOne10"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1pxv"
                android:layout_gravity="start"
                android:layout_marginTop="@dimen/_14pxv"
                android:background="@color/bluegray_50" />

            <LinearLayout
                android:id="@+id/linearRowtitle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:gravity="start"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/clubimage1"
                    cornerRadius="@{@dimen/_8pxh}"
                    imageRes="@{@drawable/img_image}"
                    android:layout_width="@dimen/_72pxh"
                    android:layout_height="@dimen/_90pxh"
                    android:scaleType="fitXY"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_image" />

                <LinearLayout
                    android:id="@+id/linearColumntitle"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_marginBottom="@dimen/_26pxv"
                    android:layout_weight="0.71"
                    android:gravity="start"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/usernameValueText"
                        style="@style/txtIntersemibold14"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@{listtitleRowModel.txtTitle}"
                        tools:text="@string/msg_glenmarie_golf" />

                </LinearLayout>
            </LinearLayout>

            <View
                android:id="@+id/lineLineOne"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1pxv"
                android:layout_gravity="start"
                android:layout_marginTop="@dimen/_14pxv"
                android:background="@color/bluegray_50" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginBottom="@dimen/_15pxh"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/clubimage2"
                        android:layout_width="@dimen/_72pxh"
                        android:layout_height="@dimen/_90pxh"
                        android:scaleType="fitXY"
                        app:srcCompat="@drawable/img_image_1" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_marginBottom="@dimen/_26pxv"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/club2"
                            style="@style/txtIntersemibold14"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/msg_bukit_kemuning" />

                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

            <View
                android:id="@+id/lineLineOne3"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_1pxv"
                android:layout_gravity="start"
                android:layout_marginTop="@dimen/_14pxv"
                android:background="@color/bluegray_50" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginBottom="@dimen/_15pxh"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/clubimage3"
                        android:layout_width="@dimen/_72pxh"
                        android:layout_height="@dimen/_90pxh"
                        android:scaleType="fitXY"
                        app:srcCompat="@drawable/img_image_2" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_marginBottom="@dimen/_26pxv"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/club3"
                            style="@style/txtIntersemibold14"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/msg_tropicana_golf" />

                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>

            <View
                android:id="@+id/lineLineOne4"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1pxv"
                android:layout_gravity="start"
                android:layout_marginTop="@dimen/_14pxv"
                android:background="@color/bluegray_50" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginBottom="@dimen/_15pxh"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/clubimage4"
                        android:layout_width="@dimen/_72pxh"
                        android:layout_height="@dimen/_90pxh"
                        android:scaleType="fitXY"
                        app:srcCompat="@drawable/img_image_3" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_marginBottom="@dimen/_26pxv"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/club4"
                            style="@style/txtIntersemibold14"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/msg_saujana_golf" />

                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

            <View
                android:id="@+id/lineLineOne5"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1pxv"
                android:layout_gravity="start"
                android:layout_marginTop="@dimen/_14pxv"
                android:background="@color/bluegray_50" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginBottom="@dimen/_15pxh"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/clubimage5"
                        android:layout_width="@dimen/_72pxh"
                        android:layout_height="@dimen/_90pxh"
                        android:scaleType="fitXY"
                        app:srcCompat="@drawable/img_image_4" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_marginBottom="@dimen/_26pxv"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/club5"
                            style="@style/txtIntersemibold14"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/msg_klgcc_kuala_lum" />

                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

            <View
                android:id="@+id/lineLineOne6"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1pxv"
                android:layout_gravity="start"
                android:layout_marginTop="@dimen/_14pxv"
                android:background="@color/bluegray_50" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginBottom="@dimen/_15pxh"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/clubimage6"
                        android:layout_width="@dimen/_72pxh"
                        android:layout_height="@dimen/_90pxh"
                        app:srcCompat="@drawable/img_image_5" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_marginBottom="@dimen/_26pxv"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/club6"
                            style="@style/txtIntersemibold14"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/msg_the_mines_resor" />

                        <TextView
                            android:id="@+id/state6"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_9pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:text="@string/lbl_selangor"
                            android:textSize="@dimen/_12pxh" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <View
                android:id="@+id/lineLineOne7"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1pxv"
                android:layout_gravity="start"
                android:layout_marginTop="@dimen/_14pxv"
                android:background="@color/bluegray_50" />

        </LinearLayout>
    </ScrollView>

</layout>
