<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="homelogRowModel"
            type="com.pgp.app.modules.homelog.data.model.HomelogRowModel"/>
    </data>

    <FrameLayout
        android:id="@+id/theframe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <FrameLayout
                android:id="@+id/frameStackglenmariegolfOne"
                android:layout_width="@dimen/_119pxh"
                android:layout_height="@dimen/_187pxv"
                android:layout_gravity="right"
                android:layout_marginRight="@dimen/_10pxv">

                <ImageView
                    android:id="@+id/imageImageTwo"
                    cornerRadius="@{@dimen/_9pxh}"
                    imageRes="@{@drawable/img_image}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:scaleType="fitXY"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_image" />

                <TextView
                    android:id="@+id/txtGlenmarieGolfOne"
                    style="@style/txtIntersemibold15"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|end"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:gravity="top|start"
                    android:text="@{homelogRowModel.txtGlenmarieGolfOne}"
                    tools:text="@string/msg_glenmarie_golf" />

            </FrameLayout>

            <FrameLayout
                android:id="@+id/example6"
                android:layout_width="@dimen/_119pxh"
                android:layout_height="@dimen/_187pxv"
                android:layout_marginRight="@dimen/_10pxh">

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/img_image_5" />

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:layout_marginStart="@dimen/_10pxv"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:fontFamily="@font/interbold"
                    android:text="@string/msg_the_mines_resor"
                    android:textColor="@color/white" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/example3"
                android:layout_width="@dimen/_119pxh"
                android:layout_height="@dimen/_187pxv"
                android:layout_marginRight="@dimen/_10pxv">

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/img_image_2" />

                <TextView
                    android:id="@+id/textTropicana"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:fontFamily="@font/interbold"
                    android:text="@string/msg_tropicana_golf"
                    android:textColor="@color/white"
                    android:textSize="14dp" />
            </FrameLayout>

        </LinearLayout>

    </FrameLayout>
</layout>
